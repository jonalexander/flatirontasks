<%= form_for @student do |f| %>

<div class="actions">
  <%= f.label :name %>
  <%= f.text_field :name %><br><br>

  <%= f.label :email %>
  <%= f.text_field :email %><br><br>

  <%= f.label :password %>
  <%= f.text_field :password %><br><br>

  <%= f.label :cohort %>
  <%= f.collection_select :cohort_id, Cohort.all, :id, :name %>
  <%= f.submit %>
  </div>
<% end %>
