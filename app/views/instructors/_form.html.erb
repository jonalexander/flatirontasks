<%= form_for @instructor do |f| %>
  <%= f.label :name %>:
  <%= f.text_field :name %><br />
<br>
  <%= f.label :email %>:
  <%= f.text_field :email %><br />
<br>
  <%= f.label :password %>:
  <%= f.text_field :password %><br />
<br>
  Password Confirmation: <%= f.password_field :password_confirmation %> <br>
<br>
  <%= f.label :cohort %>:
  <%= f.collection_select :id, Cohort.all, :id, :name %>
  <br><br>
  <%= f.submit %>
 <% end %>