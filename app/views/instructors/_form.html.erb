<%= form_for @instructor do |f| %>
  <%= f.label :name %>:
  <%= f.text_field :name %><br />

  <%= f.label :email %>:
  <%= f.text_field :email %><br />

  <%= f.label :password %>:
  <%= f.text_field :password %><br />

  <%= f.label :cohort %>:
  <%= f.collection_select :id, Cohort.all, :id, :name %>
  <br><br>
  <%= f.submit %>
 <% end %>