<h3>Here is your account!</h3>

<h3><%=@instructor.name.capitalize%></h3>

<h3> Your cohorts </h3>
<ul>
<%@instructor.cohorts.each do |cohort| %>
 <li><%=link_to cohort.name, cohort_path(cohort) %></li>
<%end%>
</ul>


<h3>Assignments you've assigned</h3>
<ul>
<%@instructor.assignments.each do | assignment|%>
<li><%=link_to assignment.name, assignment_path(assignment) %></li>
<%end%>
</ul>
