<div class="panel panel-default" id="previous_assignments">
  <div class="panel-heading"><h4>Previous Assignments</h4></div>
  <table class="table table-hover">
  <tbody>
<% @cohort.assignments.each do |assignment| %>
  <tr>
    <td><%= link_to assignment.name, "/assignments/status/#{assignment.id}/#{@cohort.id}"%></td>
  <% end %>
  </tr>
</tbody>
</table>
</div>
